    "src/**/*.{ts,tsx}": [
      "eslint --fix \"src/**/*.{ts,tsx}\""
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
